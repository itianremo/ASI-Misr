<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsTableInfo" targetNamespace="http://www.tempuri.org/dsTableInfo.xsd" xmlns:mstns="http://www.tempuri.org/dsTableInfo.xsd" xmlns="http://www.tempuri.org/dsTableInfo.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsTableInfo" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="GEN_TableInfo">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="cTableName" type="xs:string" />
              <xs:element name="cTableDescCol" type="xs:string" minOccurs="0" />
              <xs:element name="cTablePkCol" type="xs:string" />
              <xs:element name="cTableDescription" type="xs:string" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:GEN_TableInfo" />
      <xs:field xpath="mstns:cTableName" />
    </xs:unique>
  </xs:element>
</xs:schema>