<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsAddress" targetNamespace="http://www.tempuri.org/dsAddress.xsd" xmlns:mstns="http://www.tempuri.org/dsAddress.xsd" xmlns="http://www.tempuri.org/dsAddress.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsAddress" msdata:IsDataSet="true">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="GEN_Address">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="AddressID" type="xs:int" />
              <xs:element name="ContactID" type="xs:int" minOccurs="0" />
              <xs:element name="CityID" type="xs:int" minOccurs="0" />
              <xs:element name="AddressLine" type="xs:string" />
              <xs:element name="PostalCode" type="xs:string" minOccurs="0" />
              <xs:element name="PrimaryAddress" type="xs:boolean" minOccurs="0" />
              <xs:element name="AddressLine2" type="xs:string" minOccurs="0" />
              <xs:element name="ZipCode" type="xs:string" />
              <xs:element name="AddressArea" type="xs:string" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:GEN_Address" />
      <xs:field xpath="mstns:AddressID" />
    </xs:unique>
  </xs:element>
</xs:schema>