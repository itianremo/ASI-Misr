<?xml version="1.0" standalone="yes"?>
<xs:schema id="dsProjects" targetNamespace="http://www.tempuri.org/dsProjects.xsd" xmlns:mstns="http://www.tempuri.org/dsProjects.xsd" xmlns="http://www.tempuri.org/dsProjects.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="dsProjects" msdata:IsDataSet="true">
    <xs:complexType>
